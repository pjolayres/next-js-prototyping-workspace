{
    "env": {
        "browser": true
    },
    "plugins": [
        "jsx-a11y",
        "react"
    ],
    "rules": {
        "react/forbid-prop-types": "off",
        "jsx-a11y/href-no-hash": "off",
        "jsx-a11y/anchor-is-valid": ["warn", {
            "aspects": ["invalidHref"]
        }]
    }
}
